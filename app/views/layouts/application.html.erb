<!DOCTYPE html>
<html>
<head>
  <title>
    <%= content_for?(:title) ? yield(:title) : 'Petrovsky Apart House' %>
  </title>
  <%= stylesheet_link_tag "application", media: "all", "data-turbolinks-track" => true %>
  <%#= stylesheet_link_tag "responsive" %>
  <%#= stylesheet_link_tag "component" %>
  <%#= stylesheet_link_tag "bookblock" %>
  <%= render 'shared/javascripts' %>
  <%= csrf_meta_tags %>
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
</head>
<body>
  <div id="page-preloader"></div>
  <% if content_for?(:presentation) %>
  <%= yield :presentation %>
<% else %>
  <% if @page.slug=="floor_plans" %>
    <div class="fasad-bg-wrap">
      <div class="fasad-2 bg-item"></div>
      <div class="fasad bg-item"></div>
    </div>
  <% end %>
  <% if @page.slug=="contacts" %>
    <div class="bg contact"></div>
  <% end %>
  <% if @page.slug=="place" %>
    <div class="bg place" id="bg-place"></div>
  <% end %>
  <% if @page.slug=="service" %>
    <div class="bg service"></div>
  <% end %>
  <% if @page.slug=="gallery" %>
    <div class="bg gallery"></div>
  <% end %>  
  <% if @page.slug=="concept" %>
    <div class="bg concept"></div>
  <% end %>
  <% if @page.slug=="penthouses" %>
    <div class="bg penthouse"></div>
  <% end %>
<% end %>
<%= render 'shared/callback' %>
<%= render 'shared/action_info' %>
<%= render 'shared/apart_info' %>
<%= render 'shared/apart_sold' %>
<% if @page.slug=="floor_plans" %>
<div id="canvas-container" style="position: absolute;">
</div>
    <section id="house-controls-container" class="hidden">
      <%= link_to 'ВЕРНУТЬСЯ', '#', class: 'back-to-house' %>
      <!--<%= link_to 'ПОКАЗАТЬ 3D', '#', :class => 'toggle-dimensions', 'data-toggle-direction' => 'to-3d' %> -->
    </section>
<% end %>
<div id="wrapper">
	<!--CONTENT-->
  <% if @page.slug=="place" %>
    <div id="map"></div>
  <% end %>
	<div id="header">

		<div class="fl">

			<div class="logo"><a href="/"><img src="images/logo.png" alt="" /></a></div>
      <%= render 'shared/menu' %>
		</div>

    <div class="fr">
      <div class="phone" id="call_phone_1">
        8 (495) 726-72-16
      </div>
      <div class="callback">
        <a href="">ЗАКАЗАТЬ ЗВОНОК</a>
      </div>
      <div class="music">
        <a class="play_music" href="#" id="mp3button" onclick="mp3_start ()">
          </a>
      </div>
      <div class="camera">
        <a class="" onclick="reset_animation()" href="/"></a>
      </div>
    </div>

	</div>
  <% if @page.slug=="gallery" %>
	  <div id="gallery-wrap">
  <% elsif @page.slug=="floor_plans"%>
  <script type="text/javascript">
    $(window).resize(resize_1920_left);
    $(document).ready(function(){
      $('#main').show();
      $.app.apartments = JSON.parse('<%= raw @apartments_json %>');
    });
  </script>
